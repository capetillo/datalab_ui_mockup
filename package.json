{
  "name": "datalab",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bulma": "sass --load-path=node_modules src/assets/css/datalab_main.scss src/assets/css/datalab_main.css",
    "start": "npm run build-bulma -- --watch"
  },
  "dependencies": {
    "@geoman-io/leaflet-geoman-free": "^2.16.0",
    "@mdi/font": "5.9.55",
    "bulma": "^1.0.2",
    "core-js": "^3.8.3",
    "d3": "^7.9.0",
    "leaflet": "^1.9.4",
    "lru-cache": "^10.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "roboto-fontface": "*",
    "serve": "^14.2.1",
    "vue": "^3.2.13",
    "vue-router": "^4.2.5",
    "vue3-carousel": "^0.3.1",
    "vuetify": "^3.0.0-beta.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^9.20.1",
    "sass": "^1.77.8",
    "sass-loader": "^14.1.0",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "webpack-plugin-vuetify": "^2.0.0-alpha.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
